                  "componentInputParameter": "accelerator_count"
                },
                "accelerator_type": {
                  "componentInputParameter": "accelerator_type"
                },
                "args": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "[\"--epochs=2\", \"--per_replica_batch_size=128\", \"--validation_table=jk-mlops-dev.chicago_taxi_training.training_split\", \"--training_table=jk-mlops-dev.chicago_taxi_training.validation_split\"]"
                    }
                  }
                },
                "command": {
                  "runtimeValue": {
                    "constantValue": {
                      "stringValue": "[\"python\", \"train.py\"]"
                    }
                  }
                },
                "container_uri": {
                  "componentInputParameter": "training_container"
                },
                "machine_type": {
                  "componentInputParameter": "machine_type"
                },
                "project": {
